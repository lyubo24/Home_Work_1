using System;

namespace PrintFirstTenMembers
{
    class Program
    {
        static void Main(string[] args)
        {
            for (int i = 2; i < 12; i++)
            {
                if (i % 2 != 0)
                {
                    Console.Write("-");
                }
                Console.WriteLine(i);
            }
            Console.WriteLine();
        }
    }
}
